<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="project_role_project_manager" model="project.role">
            <field name="name">Project Manager</field>
            <field name="sequence">1</field>
        </record>

        <record id="project_stage_to_do" model="project.stage">
            <field name="sequence">10</field>
            <field name="name">To Do</field>
        </record>

        <record id="project_stage_in_progress" model="project.stage">
            <field name="sequence">20</field>
            <field name="name">In Progress</field>
        </record>

        <record id="project_stage_done" model="project.stage">
            <field name="sequence">30</field>
            <field name="name">Done</field>
            <field name="fold" eval="True"/>
        </record>

        <record id="project_stage_canceled" model="project.stage">
            <field name="sequence">5</field>
            <field name="name">Cancelled</field>
            <field name="fold" eval="True"/>
        </record>
    </data>
</odoo>
